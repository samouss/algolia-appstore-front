{
  "presets": [
    ["es2015", { "modules": false }],
    "es2016",
    "es2017",
    "react",
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "test": "./test",
      },
    }],
    "transform-object-rest-spread",
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
      ],
    },
  },
}
